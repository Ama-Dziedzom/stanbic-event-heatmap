# Mapbox Configuration
NEXT_PUBLIC_MAPBOX_TOKEN=your_mapbox_token_here

# Airtable Configuration
# Get your API key from: https://airtable.com/create/tokens
AIRTABLE_API_KEY=your_airtable_api_key_here

# Get your Base ID from your Airtable base URL
# URL format: https://airtable.com/appXXXXXXXXXX/...
# The Base ID is the part that starts with "app"
AIRTABLE_BASE_ID=appXXXXXXXXXX

# Your table name in Airtable (default: "Events")
AIRTABLE_TABLE_NAME=Events
